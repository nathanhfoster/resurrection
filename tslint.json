{
    "extends": [
        "tslint:latest",
        "tslint-react",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "rules": {
        "no-implicit-dependencies": [
            false,
            "dev"
        ],
        "no-submodule-imports": [
            false
        ],
        "prefer-for-of": false
    }
}